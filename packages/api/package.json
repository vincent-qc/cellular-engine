{
  "name": "@cellular/gemini-api",
  "version": "0.0.1",
  "description": "Express middleware for Gemini CLI core functionality",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "test": "tsx src/test.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@google/gemini-cli-core": "file:../core",
    "express": "^4.21.2",
    "reflect-metadata": "^0.1.13",
    "cors": "^2.8.5",
    "routing-controllers": "^0.11.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.23",
    "@types/node": "^24.0.7",
    "nodemon": "^3.0.0",
    "ts-node": "^10.9.0",
    "tsx": "^4.20.3",
    "typescript": "^5.0.0"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts,js,json",
    "ignore": [
      "src/**/*.test.ts"
    ],
    "exec": "tsx src/server.ts"
  }
}
